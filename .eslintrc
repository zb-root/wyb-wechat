{
  "extends": "standard",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "eqeqeq": "off",
    "spaced-comment": "off",
    "new-cap": "off",
    "block-spacing": "off",
    "padded-blocks": "off",
    "no-unused-vars": "off",
    "camelcase": "off",
    "handle-callback-err": "off",
    "no-template-curly-in-string": "off"
  }
}